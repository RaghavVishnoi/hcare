<%= form_for @home_workout, html: { multipart: true } do |f| %>
	 
		<div class="col-md-12">      
	    <div class="row">
	    	<div class="col-md-4">
	    			<%= f.label :Description %> 
	    		<%=f.text_field :description %>

	    	</div>
	    	<div class="col-md-1">
	    		Title
	    	</div>
	    	<div class="col-md-2">
	      		 
	      		<%=f.text_field :title %>
	    	</div>
	    	<div class="col-md-1">
	    		
	    	</div>
	    	<div class="col-md-3">
	    		
	      		<%= f.file_field :home_workout %>
	    	</div>
	    	<div class="col-md-1">
	    		 <%= f.submit "Upload" ,:class=> 'btn-success'  %>
	    	</div>
	    </div>
	</div>

	   
 	     
	   
<% end %>